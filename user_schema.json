{
  "$id": "https://example.com/user-schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "User Schema",
  "type": "object",
  "properties": {
    "birthdate": {
      "type": "string",
      "format": "date-time"
    },
    "created_time": {
      "type": "string",
      "format": "date-time"
    },
    "display_name": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "photo_url": {
      "type": "string",
      "format": "uri"
    },
    "uid": {
      "type": "string",
      "pattern": "^[A-Za-z0-9_-]{20,}$"
    }
  },
  "required": ["birthdate", "created_time", "display_name", "email", "photo_url", "uid"],
  "additionalProperties": false
}
